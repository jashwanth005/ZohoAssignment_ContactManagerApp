<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ZohoContactApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.ZohoContactApp.LoginActivity"/>
        <activity
            android:name="com.example.ZohoContactApp.HomeActivity"/>
        <activity android:name="com.example.ZohoContactApp.EditorActivity" />
        <activity android:name="com.example.ZohoContactApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="com.example.ZohoContactApp.Provider"
            android:authorities="com.example.ZohoContactApp"
            android:exported="false" />
    </application>

</manifest>